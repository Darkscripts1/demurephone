getgenv().AuthKey = "HUGE_3C4qcDEzpSth"
getgenv().LoadSettings = {
    disable_globalhatch = false,
    loadFromEnv = false,
    ignoreSave = false,
    allowMultiExecution = false,
    ToggleBind = Enum.KeyCode.RightBracket,
    loadConfig = "CONFIG_NAME_HERE" -- Will not load unless the Config Exists
}

loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/3acbf8ea627f090532fc8af027438e17.lua"))()

wait(60)

local Lib = getgenv().hgLib
if not Lib.Network.InvokeOld then Lib.Network.InvokeOld = Lib.Network.Invoke end
Lib.Network.Invoke = function(Remote, ...)
    if Remote == "CustomEggs_Hatch" then
        local Args = {...}
        Args[2] = Lib.EggCmds.GetMaxHatch()
        return Lib.Network.InvokeOld(Remote, table.unpack(Args))
    end
    
    return Lib.Network.InvokeOld(Remote, ...)
end
